<h1>リーグ</h1>
<h2>順位</h2>
<h4>2017 - 2018 season</h4>
<div class="conference"><%= link_to("カンファレンス","/teams/conference") %></div>
  <div class="teams-contents-index">
    <table class="table-design tablelock1">
      <thead>
        <tr>
          <th></th>
          <th>W</th>
          <th>L</th>
          <th>WIN%</th>
        </tr>
      </thead>
           <% @teams.each.with_index(1) do |team, i| %>
      <tbody>
        <tr>
          <td><div class="ranking"><%= i %> <%= link_to(team.name,"/teams/#{team.id}")%></div></td>
          <td><%= team.w %></td>
          <td><%= team.l %></td>
          <td><%= "#{team.win}%" %></td>
        </tr>
      </tbody>
           <% end %>
    </table>
  </div>