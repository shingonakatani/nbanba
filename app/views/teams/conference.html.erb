<h1>カンファレンス</h1>
<h2>順位</h2>
<h4>2017 - 2018 season</h4>
<h4><div class="league"><%= link_to("リーグ","/teams/index") %></div></h4>

  <div class="teams-contents-index">
     <div=class="east"><h4>イースタン カンファレンス</h4></div>
    
    <table class="table-design tablelock1">
      <thead>
        <tr>
          <th>Team</th>
          <th>W</th>
          <th>L</th>
          <th>WIN%</th>
        </tr>
      </thead>
           <% @teams_east.each.with_index(1) do |teams_east, i| %>
      <tbody>
        <tr>
          <td><div class="ranking"><%= i %> <%= link_to(teams_east.name,"/teams/#{teams_east.id}")%></div></td>
          <td><%= teams_east.w %></td>
          <td><%= teams_east.l %></td>
          <td><%= "#{teams_east.win}%" %></td>
        </tr>
      </tbody>
           <% end %>
    </table>
  </div>
  
    <div class="teams-contents-index">
      <div=class="east"><h4>ウェスタンカンファレンス</h4></div>
    <table>
      <thead>
        <tr>
          <th>Team</th>
          <th>W</th>
          <th>L</th>
          <th>WIN%</th>
        </tr>
      </thead>
           <% @teams_west.each.with_index(1) do |teams_west, i| %>
